{
  "name": "internal-react-charts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "node-sass": "^4.5.3",
    "react-scripts": "1.0.10",
    "recharts": "^1.0.0-alpha.1",
    "sass": "^1.0.0-beta.1"
  },
  "scripts": {
    "start": "npm run build:css && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "autoprefix": "postcss --use autoprefixer -r public/main.css",
    "build:scss": "node-sass src/index.scss public/main.css",
    "build:css": "npm run build:scss && npm run autoprefix",
    "watch": "npm run build:css && node-sass -w src/**/*.scss -or public/main.css"
  }
}

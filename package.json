{
  "name": "internal-react-charts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "breakpoint-sass": "^2.7.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-jshint": "^2.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "jshint": "^2.9.5",
    "node-sass": "^7.0.0",
    "postcss": "^6.0.11",
    "postcss-cli": "^4.1.1",
    "react-scripts": "1.0.10",
    "recharts": "^1.0.0-apha.5",
    "sass": "^1.0.0-beta.2"
  },
  "scripts": {
    "start": "npm run build:css && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "autoprefix": "postcss --use autoprefixer -r public/main.css",
    "build:scss": "node-sass src/main.scss public/main.css",
    "build:css": "npm run build:scss && npm run autoprefix",
    "watch": "npm run build:css && node-sass -w src/**/*.scss -or public/main.css"
  }
}
